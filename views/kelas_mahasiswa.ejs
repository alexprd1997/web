<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>LIST</title>
    <link rel="stylesheet" href="/css/style.css">
    <script src="/send_url.js"></script>
  </head>
  <body>
    <header>
      <a href="/top" class="header-left">LIST</a>
    </header>
    <div class="container">
      <div class="index-table-wrapper">
        <div class="table-head">
            <span class="id-column">ID</span>
            <span class="id-column">Nama</span>
            <span class="id-column">Kelas</span>
          </div>
        <ul class="table-body">
          <% result.forEach((hasil) => { %>
            <li>
              <div class="item-data">
                <span class="id-column"><%= hasil.nim %></span>
                <span class="id-column"><%= hasil.nama %></span>
                <span class="id-column"><%= hasil.kelas %></span>
              </div>
            </li>
          <% }) %>
        </ul>
      </div>
      <a href="/kelas" class="cancel-button">Kembali</a>
    </div>
  </body>
</html>